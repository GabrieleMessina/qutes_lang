{
  "name": "Qutes",
  "scopeName": "source.qutes",
  "patterns": [
    {
      "name": "keyword.control.qutes",
      "match": "\\b(if|else|while|do|return|in)\\b"
    },
    {
      "name": "keyword.operator.qutes",
      "match": "\\b(not|pauliy|pauliz|mcz|hadamard|measure|print|barrier|barrier|barrier|barrier|barrier|barrier|)\\b"
    },
    {
      "name": "keyword.declaration.qutes",
      "match": "\\b(qustring|quint|qubit|int|string|bool)\\b"
    },
    {
      "name": "punctuation.terminator.qutes",
      "match": ";"
    },
    {
      "name": "support.constant.qutes",
      "match": "(\\|0>|\\|1>|\\|+>\\|->)"
    },
    {
      "name": "constant.numeric.qutes",
      "match": "[0-9]*[q]?"
    },
    {
      "name": "constant.language.qutes",
      "match": "(true|false)"
    },
    {
      "name": "punctuation.definition.block.qutes",
      "begin": "\\{",
      "end": "\\}"
    },
    {
      "name": "punctuation.definition.parameters.qutes",
      "begin": "\\(",
      "end": "\\)"
    },
    {
      "name": "variable.other.qutes",
      "match": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
    },
    {
      "name": "keyword.operator.assignment.qutes",
      "match": "="
    }
  ]
}